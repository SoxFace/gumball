<%= form_for @book do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, :class => 'form-control', :placeholder => 'your title' %>
  </div>
  <div class="form-group">
    <%= f.label :cuisine %>
    <%= f.select :cuisine, Book.cuisines, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :image_url, 'bam for jam' %>
    <%= f.text_field :image_url, :class => 'form-control' %>
    <p class="help-block">This is actually the image URL.</p>
  </div>
  <button class="btn btn-primary">booook</button>

<% end %>

